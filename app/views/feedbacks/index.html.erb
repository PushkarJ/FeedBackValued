<h1>Listing feedbacks</h1>

<table>
  <tr>
    <th>Feed</th>
    <th>Name</th>
    <th>Email</th>
    <th>Contactno</th>
    <th>Customer</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @feedbacks.each do |feedback| %>
  <tr>
    <td><%= feedback.feed %></td>
    <td><%= feedback.name %></td>
    <td><%= feedback.email %></td>
    <td><%= feedback.contactno %></td>
    <td><%= feedback.customer_id %></td>
    <td><%= link_to 'Show', feedback %></td>
    <td hidden="true"><%= link_to 'Edit', edit_feedback_path(feedback) %></td>
    <td hidden="true"><%= link_to 'Destroy', feedback, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Feedback', new_feedback_path %>
